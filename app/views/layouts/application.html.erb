<!DOCTYPE html>
<html>
  <head>
    <title>LendConnect</title>
    <%= csrf_meta_tags %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" href="../../../../favicon.ico">
    <title>Dashboard Template for Bootstrap</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="row">
      <div class="col-sm-2">
      </div>
      <div class="col-sm-9">
        <div class="row">
          <div class="col-sm-2">
           <p> <h1>Mining</h1></p>
           <p><h1>Logo</h1></p>
          </div>
          <div class="col-sm-2">
            <div class="page-header">
              <p><h6>BTC</h6></p>
              <p><h6>DAV</h6></p>
              <p><h6>BCC</h6></p>
              <p><h6>LTC</h6></p>
            </div>
          </div>
          <div class="col-sm-2">
            <div class="page-header">
              <p><h6>HXT</h6></p>
              <p><h6>UCO</h6></p>
              <p><h6>REGAL</h6></p>
              <p><h6>ETH</h6></p>
            </div>
          </div>
        </div>
      </div>
    </div>
    <hr>
    <div class="container-fluid">
      <div class="row">
        <nav class="col-md-2 d-none d-md-block bg-light sidebar">
          <div class="sidebar-sticky">
            <h4>My Models</h4>
            <ul class="nav flex-column">
            <% @user_scenarios.each do |user_scenario| %>
              <li class="nav-item", style="list-style: none;">
                <span data-feather="home"></span>
                  <%= link_to "Scenario #{user_scenario.id}", edit_user_scenario_path(user_scenario), class: 'nav-link active' %> <span class="sr-only">(current)</span>
              </li>
            </ul>
            <% end %>
          </div>
        </nav>
        <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
          <%= yield %>
        </main>
      </div>
    </div>
  	<%= render 'shared/alerts' %>
  </body>
</html>
